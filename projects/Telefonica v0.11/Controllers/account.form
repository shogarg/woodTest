<EntityForm xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:dummy="0" flexi="0" fleximode="3" fullscreen="0" resize="0" version="2">
  <IsContactForm>false</IsContactForm>
  <EntityName>account</EntityName>
